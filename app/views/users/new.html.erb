
<%= form_for User.new ,:url => users_signup_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.label :role %>
  <%= f.text_field :role %>

  <%= f.submit "Submit" %>
<% end %>